DirectoryIndex index.php

RewriteEngine on
RewriteCond $1 !^(index\.php|assets|robots\.txt)

RewriteRule ^(.*)$ /index.php?$1 [L]
# DO NOT REMOVE THIS LINE AND THE LINES BELOW SSL_REDIRECT:imphalangels.com
RewriteEngine on
RewriteCond %{HTTPS} off
RewriteCond %{HTTP_HOST} ^imphalangels.com$
RewriteRule (.*) https://%{HTTP_HOST}%{REQUEST_URI}
# DO NOT REMOVE THIS LINE AND THE LINES ABOVE SSL_REDIRECT:imphalangels.com
